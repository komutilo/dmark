{
  "name": "dmark",
  "version": "1.0.1",
  "description": "Terraform wrapper for multi-stage and multi-stack which only add a config file. Perfect for no HCP projects.",
  "main": "src/index.js",
  "types": "./types/**/*.d.ts",
  "bin": {
    "dmark": "src/index.js"
  },
  "scripts": {
    "lint-script": "node scripts/nodejs/lint.js",
    "lint": "pnpm lint-script",
    "lint-fix": "pnpm lint-script --fix",
    "test-script": "node scripts/nodejs/test.js",
    "test": "pnpm test-script",
    "test-coverage": "pnpm test-script --coverage",
    "test-watch": "pnpm test-script --watch",
    "test-serve-report": "cross-env serve ./coverage/lcov-report",
    "types": "tsc"
  },
  "keywords": [
    "terraform",
    "tf",
    "iac",
    "stacks",
    "stages",
    "demark",
    "demarcation",
    "parametrize",
    "parameterizable",
    "automation",
    "wrapper",
    "dry"
  ],
  "author": {
    "name": "Bruno Araujo",
    "email": "bruno@komutilo.com",
    "url": "https://github.comn/brunurd"
  },
  "contributors": [
    {
      "name": "Bruno Araujo",
      "email": "bruno@komutilo.com",
      "url": "https://github.comn/brunurd"
    }
  ],
  "homepage": "https://github.com/komutilo/dmark#readme",
  "bugs": {
    "url": "https://github.com/komutilo/dmark/issues",
    "email": "bruno@komutilo.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/komutilo/dmark.git"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "4.1.0",
    "columnify": "^1.6.0",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "fs-extra": "^10.1.0",
    "js-yaml": "^4.1.0",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "yaml-schema-validator": "^1.2.3",
    "promisify-child-process": "^4.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.8.3",
    "eslint": "^8.23.1",
    "eslint-plugin-security": "^1.5.0",
    "jest": "^29.1.2",
    "jest-junit": "^14.0.1",
    "mock-argv": "^2.0.8",
    "serve": "^14.0.1",
    "spawk": "^1.7.1",
    "typescript": "^4.8.3"
  }
